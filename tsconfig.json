{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/apps/organization-admin/components/*": ["./src/apps/organization-admin/components/*"],
      "@/apps/saas-admin/components/*": ["./src/apps/saas-admin/components/*"],
      "@/apps/supplymantix/components/*": ["./src/apps/supplymantix/components/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
